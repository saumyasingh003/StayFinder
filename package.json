{
  "name": "shelter-app",
  "version": "1.0.0",
  "description": "Full-stack application with React frontend and Node.js backend",
  "scripts": {
    "install:frontend": "cd CozyCrib && npm install",
    "install:backend": "cd Backend && npm install",
    "build:frontend": "cd CozyCrib && npm run build",
    "start:backend": "cd Backend && npm start",
    "deploy": "npm run install:frontend && npm run install:backend && npm run build:frontend",
    "dev:frontend": "cd CozyCrib && npm run dev",
    "dev:backend": "cd Backend && npm start"
  },
  "keywords": ["react", "vite", "nodejs", "fullstack"],
